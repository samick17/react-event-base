{
  "name": "react-event-base",
  "version": "1.0.6",
  "description": "React event-driven library",
  "author": "samick17",
  "license": "MIT",
  "repository": "samick17/react-event-base",
  "keywords": [
    "React",
    "ReactJS",
    "EventDriven",
    "CodeBase",
    "Drag and Drop"
  ],
  "main": "index.js",
  "module": "index.es.js",
  "jsnext:main": "index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "sh ./build.sh",
    "build:main": "rollup -c rollup-configs/Main.rollup.config.js",
    "build:component": "rollup -c rollup-configs/Components.rollup.config.js",
    "build:core": "rollup -c rollup-configs/Core.rollup.config.js",
    "build:dnd": "rollup -c rollup-configs/DragAndDrop.rollup.config.js",
    "build:utils": "rollup -c rollup-configs/DOMUtils.rollup.config.js && rollup -c rollup-configs/EventUtils.rollup.config.js && rollup -c rollup-configs/IOUtils.rollup.config.js && rollup -c rollup-configs/ObjectUtils.rollup.config.js && rollup -c rollup-configs/PerfUtils.rollup.config.js && rollup -c rollup-configs/VK.rollup.config.js",
    "build:mathf": "rollup -c rollup-configs/Mathf.rollup.config.js",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "prepublishOnly": "cp -r ./dist/* . && rm -rf ./dist",
    "postpublish": "git clean -fd"
  },
  "peerDependencies": {
    "html2canvas": "^1.0.0-rc.5",
    "js-md5": "^0.7.3",
    "lzutf8": "^0.5.5",
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-url": "^4.0.2",
    "@svgr/rollup": "^5.2.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "^3.4.0",
    "rollup": "^1.32.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^2.1.1"
  }
}
